<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="imerir.CDLMR.appRobotClientSide.view.BasicIHMController">
   <children>
      <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <AnchorPane BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="filePathLabel" text="Please choose a file" AnchorPane.leftAnchor="5.0" />
               </children></AnchorPane>
         </top>
         <bottom>
            <AnchorPane BorderPane.alignment="CENTER">
               <children>
                  <ButtonBar layoutX="286.0" layoutY="146.0" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="5.0">
                    <buttons>
                        <Button fx:id="chooseFileButton" mnemonicParsing="false" onAction="#handleChooseFileButton" text="Select File" />
                        <Button fx:id="impressionButton" mnemonicParsing="false" onAction="#handleImpressionButton" text="Impression" />
                    </buttons>
                  </ButtonBar>
               </children></AnchorPane>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
